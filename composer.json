{
    "name": "amphp/green-thread",
    "homepage": "http://amphp.org",
    "description": "Green thread implementation based on ext-fiber extension",
    "keywords": [
        "async",
        "asynchronous",
        "concurrency",
        "green thread",
        "thread",
        "non-blocking"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Aaron Piotrowski",
            "email": "aaron@trowski.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-fiber": "*",
        "amphp/amp": "^2.5"
    },
    "require-dev": {
        "amphp/phpunit-util": "^1",
        "react/promise": "^2",
        "amphp/php-cs-fixer-config": "dev-master",
        "phpunit/phpunit": "^9",
        "vimeo/psalm": "^3.11@dev"
    },
    "autoload": {
        "psr-4": {
            "Amp\\": "src"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Amp\\GreenThread\\Test\\": "test"
        }
    },
    "support": {
        "issues": "https://github.com/amphp/green-thread/issues",
        "irc": "irc://irc.freenode.org/amphp"
    }
}
